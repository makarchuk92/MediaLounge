{
  "expo": {
    "name": "MediaLounge",
    "slug": "MediaLounge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#111"
      },
      "requireFullScreen": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
